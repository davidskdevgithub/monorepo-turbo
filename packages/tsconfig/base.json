{
	"$schema": "https://json.schemastore.org/tsconfig",
	"display": "Default",
	"compilerOptions": {
		"composite": false,
		"declaration": true,
		"declarationMap": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"inlineSources": false,
		"isolatedModules": true,
		"moduleResolution": "node",
		"noEmitOnError": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"preserveWatchOutput": true,
		"skipLibCheck": true,
		"strict": true,
		"target": "es2017",
		"lib": ["es2017", "dom"]
	}
}
